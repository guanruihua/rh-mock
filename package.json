{
  "name": "rh-mock",
  "version": "0.0.6",
  "description": "方法包",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only ./src/index.ts",
    "dev_test": "ts-node-dev --respawn --transpile-only ./src/test/index.ts",
    "test": "jest",
    "test-c": "jest --coverage",
    "restart": "rimraf dist && npm run build && npm start",
    "start": "node ./dist/index.js",
    "prod": "npm run build && npm run start",
    "pub": "./push.bat & rimraf ./dist & tsc & npm publish"
  },
  "keywords": [
    "typescript",
    "mock"
  ],
  "author": "ruihuag",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.6",
    "jest": "^27.3.1",
    "node-notifier": "^8.0.2",
    "rh-js-methods": "^0.0.7",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.7",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.4"
  }
}
